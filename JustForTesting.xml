<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Just For Testing suite">
  <parameter name="os" value="ios" />
  
  <test verbose="2" name="JFT test">
	  <parameter name="os" value="ios" />
	  <parameter name="fileName" value="LDS Tools.app" />
	  <parameter name="testDevice" value="iPhone 7" />
	     
      
      <groups>
          <run>
              <include name="jft"/>
          </run>
      </groups>

      <classes>
          <class name="LDSTest.LDSTools"/>
          <class name="LDSTest.LDSTools$Retry"/>      
      </classes>
      
  </test> <!-- Default test -->
</suite> <!-- Default suite -->